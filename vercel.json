{
  "buildCommand": "npx expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "npx expo start --web",
  "cleanUrls": true,
  "routes": [
 
    { "handle": "filesystem" },

    { "src": "/_expo/static/(.*)", "dest": "/_expo/static/$1" },

    { "src": "/images/(.*)",      "dest": "/images/$1" },

    { "src": "/(.*)",             "dest": "/index.html" }
  ]
}
