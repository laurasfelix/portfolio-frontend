{
  "buildCommand": "npx expo export -p web",
  "outputDirectory": "dist",
  "devCommand": "npx expo start --web",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/_expo/static/:path*",
      "destination": "/dist/_expo/static/:path*"
    },
    {
      "source": "/images/:path*",
      "destination": "/dist/images/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/dist/index.html"
    }
  ]
}
